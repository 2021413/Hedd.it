{
  "kind": "collectionType",
  "collectionName": "user_profiles",
  "info": {
    "singularName": "user-profile",
    "pluralName": "user-profiles",
    "displayName": "UserProfile",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "username": {
      "type": "string"
    },
    "avatar": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "banner": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "subscriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-profile.user-profile",
      "mappedBy": "followers"
    },
    "followers": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::user-profile.user-profile",
      "inversedBy": "subscriptions"
    },
    "community": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::community.community",
      "mappedBy": "members"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "author"
    },
    "comment": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::comment.comment",
      "inversedBy": "downvotes"
    },
    "posts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::post.post",
      "mappedBy": "upvote"
    }
  }
}
